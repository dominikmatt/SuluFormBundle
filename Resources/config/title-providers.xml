<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Title Provider Pool -->
        <service id="sulu_form.title_provider.pool" class="Sulu\Bundle\FormBundle\TitleProvider\TitleProviderPool">
            <argument type="collection"/>
        </service>

        <!-- Structure Title Provider -->
        <service id="sulu_form.title_provider.structure" class="Sulu\Bundle\FormBundle\TitleProvider\Collections\StructureTitleProvider">
            <tag name="sulu_form.title_provider" alias="structure"/>
            <argument type="service" id="request_stack"/>
        </service>
    </services>
</container>
